name: CI Pipeline
on: [push, pull_request]
jobs:
  test:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
- name: Run tests
  run: npm test
  working-directory: unit4-MSIT-5250-01-Foundations-of-Software-Engineering---AY2026-T1


- name: Performance Benchmark
  run: npm run benchmark-api
  working-directory: your-subfolder-name
